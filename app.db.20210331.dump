CREATE TABLE item_version (
	id INTEGER NOT NULL, 
	name VARCHAR(50), 
	label VARCHAR(20), 
	manufacturer VARCHAR(50), 
	model VARCHAR(50), 
	serial VARCHAR(50), 
	description TEXT, 
	category VARCHAR(50), 
	wikilink VARCHAR(250), 
	thumbnail INTEGER, 
	transaction_id INTEGER NOT NULL, 
	end_transaction_id INTEGER, 
	operation_type SMALLINT NOT NULL, 
	PRIMARY KEY (id, transaction_id)
);
CREATE TABLE picture_version (
	id INTEGER NOT NULL, 
	file VARCHAR(128), 
	transaction_id INTEGER NOT NULL, 
	end_transaction_id INTEGER, 
	operation_type SMALLINT NOT NULL, 
	PRIMARY KEY (id, transaction_id)
);
CREATE TABLE IF NOT EXISTS "transaction" (
	issued_at DATETIME, 
	id INTEGER NOT NULL, 
	remote_addr VARCHAR(50), 
	PRIMARY KEY (id)
);
CREATE TABLE user_version (
	id INTEGER NOT NULL, 
	username VARCHAR(64), 
	fullname VARCHAR(250), 
	password_hash VARCHAR(128), 
	email VARCHAR(128), 
	telegram_name VARCHAR(64), 
	transaction_id INTEGER NOT NULL, 
	end_transaction_id INTEGER, 
	operation_type SMALLINT NOT NULL, 
	PRIMARY KEY (id, transaction_id)
);
CREATE UNIQUE INDEX ix_item_label ON item (label);
CREATE INDEX ix_user_email ON user (email);
CREATE UNIQUE INDEX ix_user_username ON user (username);
CREATE INDEX ix_item_version_end_transaction_id ON item_version (end_transaction_id);
CREATE INDEX ix_item_version_label ON item_version (label);
CREATE INDEX ix_item_version_operation_type ON item_version (operation_type);
CREATE INDEX ix_item_version_transaction_id ON item_version (transaction_id);
CREATE INDEX ix_picture_version_end_transaction_id ON picture_version (end_transaction_id);
CREATE INDEX ix_picture_version_operation_type ON picture_version (operation_type);
CREATE INDEX ix_picture_version_transaction_id ON picture_version (transaction_id);
CREATE INDEX ix_user_version_email ON user_version (email);
CREATE INDEX ix_user_version_end_transaction_id ON user_version (end_transaction_id);
CREATE INDEX ix_user_version_operation_type ON user_version (operation_type);
CREATE INDEX ix_user_version_transaction_id ON user_version (transaction_id);
CREATE INDEX ix_user_version_username ON user_version (username);
COMMIT;
