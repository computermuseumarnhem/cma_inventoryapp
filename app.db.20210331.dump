CREATE TABLE user (
	id INTEGER NOT NULL, 
	username VARCHAR(64), 
	password_hash VARCHAR(128), 
	email VARCHAR(128), 
	telegram_name VARCHAR(64), fullname VARCHAR(250), 
	PRIMARY KEY (id)
